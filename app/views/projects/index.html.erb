<div id='portfolioindex' class='row'>
  <h2>Portfolio</h2>
  <div class='large-12 columns'>
    <div class='row'>
      <div class='large-8 columns'>
        <div id='featured' class='row'>
          <p><br></p>
        </div>
      </div>
      <div id='portfolio'>
        <ul class='large-block-grid-4'>
          <% @projects.each do |project| %>
            <%= render project %>
          <% end %>
        </ul>
        <% if current_user && current_user.admin? %>
          <%= link_to 'New Project', new_project_path, id: 'new_project_link',class: 'button', remote: true %>
        <% end %>
      </div>
    </div>
  </div>
</div>
